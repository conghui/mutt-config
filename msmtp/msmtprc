account gmail              
host smtp.gmail.com          
port 587                     
protocol smtp
auth on                     
logfile ~/.msmtp.log
syslog on
from "heconghui@gmail.com"   
user "heconghui"
tls on                       
tls_starttls on              
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account default : gmail
